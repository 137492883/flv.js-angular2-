/**
 * 本实体为内容组与内容的中转实体
 */
export class AdvertisementGroupTransient {
    /**
     * 内容id
     */
    advertisementId: string;
    /**
     * 内容名称
     */
    advertisementName: string;
    /**
     * 索引
     */
    index: number;
    /**
     * 播放时长：秒
     */
    duration: number;
    num: number;
    /**
     * 时
     */
    hour: number=0;
    /**
     * 分
     */
    minute: number=0;
    /**
     * 秒
     */
    seconds: number=0;
    h = '00';
    m = '00';
    s = '00';
}
