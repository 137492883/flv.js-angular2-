<div [@routerTransition]>
    <div class="row">
        <div class="col-xl-12">
            <ol class="breadcrumb">
                <li class="jb-warp breadcrumb-item"></li>
                <li class="breadcrumb-item "><i class="fa fa-edit"></i>
                    详细信息
                </li>
            </ol>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="card mb-3">
                <hr>
                <div class="card-body">
                    <div class="container">
                        <div class="form-group row">
                            <div class="col-sm-2 text-right">
                                <label>设备sn：</label>
                            </div>
                            <div class="col-sm">
                                {{systemLog.deviceSn}}
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-2 text-right">
                                <label>日志时间：</label>
                            </div>
                            <div class="col-sm">
                                {{systemLog.createtime}}
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-2 text-right">
                                <label>接口类型：</label>
                            </div>
                            <div class="col-sm">
                                {{systemLog.actionDes}}
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-2 text-right">
                                <label>操作结果：</label>
                            </div>
                            <div class="col-sm">
                                <span *ngIf="systemLog?.status === '操作失败'" style="color: red">{{systemLog?.status}}</span>
                                <span *ngIf="systemLog?.status !== '操作失败'">{{systemLog?.status}}</span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-2 text-right">
                                <label>参数：</label>
                            </div>
                            <div class="col-sm">
                                {{getParam(systemLog.param)}}
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-2 text-right">
                                <label>返回值：</label>
                            </div>
                            <div class="col-sm">
                                {{getParam(systemLog.res)}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
