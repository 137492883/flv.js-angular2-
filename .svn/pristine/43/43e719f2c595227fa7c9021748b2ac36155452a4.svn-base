<div [@routerTransition]>
    <app-page-header [heading]="'用户管理'" [icon]="'fa-table'"></app-page-header>
    <div class="card" style="width: 20rem;">
        <img class="card-img-top" src="..." alt="Card image cap">
        <div class="card-body">
            <h4 class="card-title">Card title</h4>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
    <div class="row" >
        <div class="col">
            <div class="card mb-3 border-success">
                <div class="card-header" style="display: flex;justify-content: space-between">
                    <span >组织架构</span>
                    <button class="btn btn-small btn-info">保存</button>
                </div>
                <div class="card-body">
                    <div class="custom-tree">
                    <tree-root [nodes]="nodes" >
                        <ng-template #treeNodeTemplate let-node let-index="index">
                            <span>{{ node.data.name }}</span>
                            <div style="padding-left: 30px;">
                                <button class="btn btn-small btn-info">新建子机构</button>
                                <button class="btn btn-small btn-link">修改</button>
                                <button class="btn btn-small btn-danger">删除</button>
                            </div>

                        </ng-template>
                    </tree-root>
                    </div>
                </div>
            </div>
        </div>

    </div>
